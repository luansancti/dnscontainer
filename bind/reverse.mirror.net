;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	dnscontainer.mirror.net. root.dnscontainer.mirror.net. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dnscontainer.mirror.net.
@   IN  PRT	mirror.net.
1   IN  PTR roteador.mirror.net.
6   IN  PTR dnscontainer.mirror.net.

;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	dnscontainer.local.alls. root.local.alls. (
                100		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

			IN	NS	dnscontainer.
1			IN  PTR roteador.local.alls.
6	 		IN  PTR mirror.local.alls.

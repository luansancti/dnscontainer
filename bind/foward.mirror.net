;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	dnscontainer.mirror.net. root.dnscontainer.mirror.net. (
                100		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

@			IN	NS	dnscontainer.mirror.net.
mirror.net. IN  A   192.168.25.6
vaio-mirror IN  A   192.168.25.6
roteador    IN  A   192.168.25.1


;@	IN	NS	localhost.
;@	IN	A	127.0.0.1
;@	IN	AAAA	::1


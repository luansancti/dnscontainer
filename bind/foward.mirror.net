;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	dnscontainer.mirror.net. root.mirror.net. (
                100		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

mirror.net.			IN	NS	dnscontainer.mirror.net.
mirror.net.			IN  A   192.168.25.6
mirror 				IN  A   192.168.25.6
dnscontainer		IN	A	192.168.25.6
roteador    		IN  A   192.168.25.1

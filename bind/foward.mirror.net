;
; BIND data file for local loopback interface
;
$TTL	604800;
@	IN	SOA	dnscontainer.mirror.net. root.mirror.net. (
                100		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

$TTL	86400;
mirror.net.						IN	NS	dnscontainer.mirror.net.
mirror.net.						IN  A   192.168.25.40

$ORIGIN mirror.net.
vaio-mirror						IN  A   192.168.25.6
dnscontainer					IN	A	192.168.25.40
roteador    					IN  A   192.168.25.1
jenkins						IN	A	192.168.25.6
portainer					IN	A	192.168.25.6
